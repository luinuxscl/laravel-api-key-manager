{
    "name": "luinuxscl/laravel-api-key-manager",
    "description": "Package para gestionar API Keys de servicios externos en Laravel.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Luinuxscl\\LaravelApiKeyManager\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luinuxscl\\LaravelApiKeyManager\\Providers\\APIKeyManagerServiceProvider"
            ]
        }
    }
}
